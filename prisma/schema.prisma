generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model violations {
  id                   String   @id @default(auto()) @map("_id") @db.ObjectId
  /// Multiple data types found: Int: 77.3%, BigInt: 22.7% out of 44 sampled entries
  v                    Json     @map("__v")
  date                 DateTime @db.Date
  latitude             Float
  license_plate_number String
  longitude            Float
  street_name          String
  time                 String
  vehicle_type         String
  violation_type       String
  zip_code             String
}
