generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model violations {
  id                   String @id @default(auto()) @map("_id") @db.ObjectId
  /// Multiple data types found: Int: 68.3%, BigInt: 31.7% out of 63 sampled entries
  v                    Json   @map("__v")
  date                 String
  latitude             Float
  license_plate_number String
  longitude            Float
  street_name          String
  time                 String
  vehicle_type         String
  violation_type       String
  zip_code             String

  @@map("Violations")
}

